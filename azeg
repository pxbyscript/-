local AbsoluteZero = loadstring(game:HttpGet("https://raw.githubusercontent.com/pxbyscript/-/refs/heads/main/azlib"))()
local Window = AbsoluteZero:CreateWindow("バージョン1", "スピード伝説")

local Tab1 = Window:CreateTab("一般")
local Tab2 = Window:CreateTab("オートファーム")
local Tab3 = Window:CreateTab("その他")

Tab1:CreateSection("セクション 1")
Tab1:CreateLabel("ラベル 1")
Tab1:CreateButton("ボタン 1", function()
	print("ボタンが動作しました！")
end)
Tab1:CreateToggle("トグル 1", function(state)
	print(state)
end)
Tab1:CreateBox("ボックス 1", function(input)
	print(input)
end)
Tab1:CreateDropdown("ドロップダウン 1", {"オプション 1","オプション 2","オプション 3"}, function(selected)
	print(selected)
end)

Tab2:CreateSection("セクション 2")
Tab2:CreateLabel("ラベル 2")
Tab2:CreateButton("ボタン 2", function()
	print("ボタンが動作しました！")
end)
Tab2:CreateToggle("トグル 2", function(state)
	print(state)
end)
Tab2:CreateBox("ボックス 2", function(input)
	print(input)
end)
Tab2:CreateDropdown("ドロップダウン 2", {"オプション 1","オプション 2","オプション 3"}, function(selected)
	print(selected)
end)
