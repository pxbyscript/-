        orbMultiplier = 1250
    elseif selected == "1300オーブ" then
        orbMultiplier = 1300
    elseif selected == "1350オーブ" then
        orbMultiplier = 1350
    elseif selected == "1400オーブ" then
        orbMultiplier = 1400
    elseif selected == "1450オーブ" then
        orbMultiplier = 1450
    elseif selected == "1500オーブ" then
        orbMultiplier = 1500
    end
end)
Tab2:CreateToggle("自動オーブ", function(state)
    if state then 
        autoOrb1 = true
        while autoOrb1 do 
            if not autoOrb1 then break end
            if pingMonitorActive then
                local ping = tonumber(string.match(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString(), "%d+"))
                if ping and ping > maxPing then
                    repeat
                        wait(1)
                        ping = tonumber(string.match(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString(), "%d+"))
                    until ping and ping < minPing or not autoOrb1
                    if not autoOrb1 then break end
                end
            end
            local batchSize = math.ceil(orbMultiplier / 5)
            local remainingOrbs = orbMultiplier
            local startTime = tick()
            while remainingOrbs > 0 do
                local currentBatch = math.min(batchSize, remainingOrbs)
                for i = 1, currentBatch do
                    game:GetService('ReplicatedStorage').rEvents.orbEvent:FireServer("collectOrb", optionOrb, optionCity)
                end
                remainingOrbs = remainingOrbs - currentBatch
                wait(0.11)
            end
            wait(orbCooldown)
        end 
    else 
        autoOrb1 = false
    end
end)
