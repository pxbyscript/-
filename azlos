    end
end)
Tab2:CreateBox("オーブの速度を入力", function(input)
    local inputNumber = tonumber(input)
    if inputNumber then
        orbMultiplier3 = inputNumber
    end
end)
Tab2:CreateToggle("自動オーブ", function(state)
    if state then 
        autoOrb3 = true
        while autoOrb3 do 
            if not autoOrb3 then break end
            if pingMonitorActive then
                local ping = tonumber(string.match(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString(), "%d+"))
                if ping and ping > maxPing then
                    repeat
                        wait(1)
                        ping = tonumber(string.match(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString(), "%d+"))
                    until ping and ping < minPing or not autoOrb3
                    if not autoOrb3 then break end
                end
            end
            local batchSize = math.ceil(orbMultiplier3 / 5)
            local remainingOrbs = orbMultiplier3
            local startTime = tick()
            while remainingOrbs > 0 do
                local currentBatch = math.min(batchSize, remainingOrbs)
                for i = 1, currentBatch do
                    game:GetService('ReplicatedStorage').rEvents.orbEvent:FireServer("collectOrb", optionOrb3, optionCity3)
                end
                remainingOrbs = remainingOrbs - currentBatch
                wait(0.11)
            end
            wait(orbCooldown)
        end 
    else 
        autoOrb3 = false
    end
end)

-- 転生ファーム
Tab3:CreateSection("転生設定")
Tab3:CreateBox("目標転生を入力", function(input)
    rebamount = tonumber(input) or 0
end)
Tab3:CreateBox("転生クールダウンを入力", function(input)
    local inputNumber = tonumber(input)
    if inputNumber then
        autoRebirthCooldown = inputNumber / 100
    else
        autoRebirthCooldown = 0.5
    end
end)

Tab3:CreateSection("転生ファーム")
Tab3:CreateToggle("自動転生", function(state)
    if state then 
        autoRebirth = true
        while autoRebirth do 
            wait()
            game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer("rebirthRequest")
            wait(autoRebirthCooldown)
        end 
    else 
        autoRebirth = false
    end 
end)
Tab3:CreateToggle("自動ターゲット転生", function(state)
    if state then 
        autoRebirthStoppingPoint = true
        while autoRebirthStoppingPoint do 
            wait()
            local success, err = pcall(function()
                local rebirthsBefore = game.Players.LocalPlayer.leaderstats.Rebirths.Value
                game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer("rebirthRequest")
                wait(autoRebirthCooldown)
                local rebirthsAfter = game.Players.LocalPlayer.leaderstats.Rebirths.Value
                if rebirthsAfter > rebirthsBefore then
                    rebirthSuccessful = true
                    wait(autoRebirthCooldown)
                    rebirthSuccessful = false
                end
                if rebirthsAfter >= rebamount then
                    autoRebirthStoppingPoint = false
                    autoRebirth = false
                    autoOrb1 = false
                    autoOrb2 = false
                    autoOrb3 = false
                end
            end)
            if not success then
                autoRebirthStoppingPoint = false
                autoRebirth = false
                autoOrb1 = false
                autoOrb2 = false
                autoOrb3 = false
            end
        end 
    else 
        autoRebirthStoppingPoint = false
        autoRebirth = false
        autoOrb1 = false
        autoOrb2 = false
        autoOrb3 = false
    end 
end)

Tab3:CreateSection("即時転生")
Tab3:CreateBox("即時転生量を入力", function(input)
    local inputNumber = tonumber(input)
    if inputNumber then
        instantRebirthAmount = inputNumber
    else
        instantRebirthAmount = 0
    end
end)
Tab3:CreateButton("起動", function()
    for i = 1, instantRebirthAmount do
        game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer("rebirthRequest")
    end
end)

