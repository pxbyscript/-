    local inputNumber = tonumber(input)
    if inputNumber then
        orbCooldown = inputNumber / 100
    else
        orbCooldown = 0.6
    end
end)
Tab2:CreateSection("最初のオーブ自動ファーム")
Tab2:CreateDropdown("オーブを選択", {"黄色オーブ","オレンジオーブ","青オーブ","赤オーブ","エーテリアルオーブ","宝石","地獄の宝石"}, function(selected)
    if selected == "黄色オーブ" then
        optionOrb = "Yellow Orb"
    elseif selected == "オレンジオーブ" then
        optionOrb = "Orange Orb"
    elseif selected == "青オーブ" then
        optionOrb = "Blue Orb"
    elseif selected == "赤オーブ" then
        optionOrb = "Red Orb"
    elseif selected == "エーテリアルオーブ" then
        optionOrb = "Ethereal Orb"
    elseif selected == "宝石" then
        optionOrb = "Gem"
    elseif selected == "地獄の宝石" then
        optionOrb = "Infernal Gem"
    end
end)
Tab2:CreateDropdown("都市を選択", {"都市","雪の都市","マグマの都市","レジェンズハイウェイ","スピードジャングル","スペース","砂漠"}, function(selected)
