Tab8:CreateSection("ワールドテレポート")  
Tab8:CreateDropdown("目的地を選択", {"シティ","スペース","デザート"}, function(dtp)
    if dtp == "シティ" then
        premiumTeleport = dtp
    elseif dtp == "スペース" then
        premiumTeleport = dtp
    elseif dtp == "デザート" then
        premiumTeleport = dtp
    end
end)
Tab8:CreateButton("テレポート", function()
    if premiumTeleport == "シティ" then
        game:GetService("TeleportService"):Teleport(3101667897)
    elseif premiumTeleport == "スペース" then
        game:GetService("TeleportService"):Teleport(3232996272)
    elseif premiumTeleport == "デザート" then
        game:GetService("TeleportService"):Teleport(3276265788)
    end
end)

Tab8:CreateSection("サーバーナビゲーション")  
Tab8:CreateDropdown("ナビゲーションタイプを選択", {"最少プレイヤー数","サーバーホップ","再接続"}, function(ptp)
    if ptp == "最少プレイヤー数" then
        premiumServerTeleport = ptp
    elseif ptp == "サーバーホップ" then
        premiumServerTeleport = ptp
    elseif ptp == "再接続" then
        premiumServerTeleport = ptp
    end
end)
Tab8:CreateButton("ナビゲート", function()
    if premiumServerTeleport == "最少プレイヤー数" then
        LowestPlayer()
    elseif premiumServerTeleport == "サーバーホップ" then
        Serverhop()
    elseif premiumServerTeleport == "再接続" then
        Rejoin()
    end
end)
