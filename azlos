Tab8:CreateSection("都市テレポート")  
Tab8:CreateDropdown("目的地を選択", {"シティ","スノーシティ","マグマシティ","レジェンズハイウェイ","スピードジャングル"}, function(input)
    if input == "シティ" then
        citiesteleport = input
    elseif input == "スノーシティ" then
        citiesteleport = input
    elseif input == "マグマシティ" then
        citiesteleport = input
    elseif input == "レジェンズハイウェイ" then
        citiesteleport = input
    elseif input == "スピードジャングル" then
        citiesteleport = input
    end
end)
Tab8:CreateButton("テレポート", function()
    if citiesteleport == "シティ" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-9682.98828, 74.8522873, 3099.03394, 0.087131381, 0, 0.996196866, 0, 1, 0, -0.996196866, 0, 0.087131381)
    elseif citiesteleport == "スノーシティ" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-9676.13867, 74.8522873, 3782.69385, 0, 0, -1, 0, 1, 0, 1, 0, 0)
    elseif citiesteleport == "マグマシティ" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-11054.9688, 232.791656, 4898.62842)
    elseif citiesteleport == "レジェンズハイウェイ" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-13098.8711, 232.791656, 5907.62793)
    elseif citiesteleport == "スピードジャングル" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-15274,399,5576)
    end
end)

