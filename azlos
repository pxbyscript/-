-- ペットの孵化
Tab4:CreateSection("基本ペットを孵化")
Tab4:CreateDropdown("ペットを選択", {"赤いウサギ","赤い子猫","青いウサギ","銀の犬",'黄色いスクイーク'}, function(input)
    if input == "赤いウサギ" then
        selectedbasicpettohatch = "Red Bunny"
    elseif input == "赤い子猫" then
        selectedbasicpettohatch = "Red Kitty"
    elseif input == "青いウサギ" then
        selectedbasicpettohatch = "Blue Bunny"
    elseif input == "銀の犬" then
        selectedbasicpettohatch = "Silver Dog"
    elseif input == "黄色いスクイーク" then
        selectedbasicpettohatch = "Yellow Squeak"
    end        
end)
Tab4:CreateToggle("自動孵化",function(state)
    if state then 
        hatchBasicPets = true 
        while hatchBasicPets == true do wait() 
            local args = { 
                [1] = game:GetService("ReplicatedStorage"):WaitForChild("cPetShopFolder"):WaitForChild(selectedbasicpettohatch)
            }
            game:GetService("ReplicatedStorage"):WaitForChild("cPetShopRemote"):InvokeServer(unpack(args)) 
        end 
    else 
        hatchBasicPets = false 
    end  
end)

