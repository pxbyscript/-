-- 絶対零度
local AbsoluteZero = loadstring(game:HttpGet("https://raw.githubusercontent.com/pxbyscript/-/refs/heads/main/azlib"))()
local Window = AbsoluteZero:CreateWindow("バージョン1", "スピード伝説")

-- サービス
local TeleportService = game:GetService("TeleportService")
local Player = game:GetService("Players").LocalPlayer

-- プレイヤー
local placeId = game.PlaceId
local player = game.Players.LocalPlayer

-- 接続最適化
local minPing = 450
local maxPing = 2500
local antiKickConnection

-- レースファーム
local race = "バージョン1"

-- トレーディング
local usernameTrade = ""

-- 転生ファーム
local rebamount = 0
local autoRebirthStoppingPointCooldown = 0.5
local autoRebirthCooldown = 0.5
local rebirthSuccessful = false

-- オーブファーム
local orbCooldown = 0.4
local orbMultiplier = 0
local optionOrb = ""
local optionCity = ""
local orbMultiplier2 = 0
local optionOrb2 = ""
local optionCity2 = ""
local orbMultiplier3 = 0
local optionOrb3 = ""
local optionCity3 = ""

-- ペットの孵化
local selectedbasicpettohatch = ""
local selectedadvancedpettohatch = ""
