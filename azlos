-- 基本ペットの進化
Tab5:CreateSection("基本ペットの進化")
Tab5:CreateDropdown("ペットを選択", {"レッドバニー","レッドキティ","ブルーバニー","シルバードッグ",'イエロースクイーク'}, function(input)
    if input == "レッドバニー" then
        selectedbasicpettoevolve = "Red Bunny"
    elseif input == "レッドキティ" then
        selectedbasicpettoevolve = "Red Kitty"
    elseif input == "ブルーバニー" then
        selectedbasicpettoevolve = "Blue Bunny"
    elseif input == "シルバードッグ" then
        selectedbasicpettoevolve = "Silver Dog"
    elseif input == "イエロースクイーク" then
        selectedbasicpettoevolve = "Yellow Squeak"
    end
end)
Tab5:CreateToggle("自動進化",function(state)
    if state then 
        evolveBasicPets = true 
        while evolveBasicPets == true do wait() 
            local args = {
                [1] = "evolvePet",
                [2] = selectedbasicpettoevolve
            }
            game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("petEvolveEvent"):FireServer(unpack(args)) 
        end 
    else 
        evolveBasicPets = false 
    end  
end)

